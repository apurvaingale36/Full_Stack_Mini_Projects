<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="com.hibernate_que\.Alien" table="items">
 
		<id name="aid" type="int">
			<column name="aid" />
			<generator class="native" />
		</id>
 
		<property name="name" type="string">
			<column name="name" />
		</property>
 
		<property name="acolor" type="string">
			<column name="acolor" />
		</property>
	</class>
</hibernate-mapping>